<!DOCTYPE html>
<html lang="zh-CN">
	<head>
		<title><%= title %></title>
		<meta charset="utf-8">
		<meta content="" name="description">
		<meta content="" name="keywords">
		<meta content="eric.wu" name="author">
		<meta content="application/xhtml+xml;charset=UTF-8" http-equiv="Content-Type">
		<!-- <meta content="no-cache,must-revalidate" http-equiv="Cache-Control">
		<meta content="no-cache" http-equiv="pragma">
		<meta content="0" http-equiv="expires"> -->
		<meta content="telephone=no, address=no" name="format-detection">
		<meta content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" name="viewport">
		<link href="/css/reset.css?v=<%= config.version %>" rel="stylesheet" />
		<link href="/css/common.css?v=<%= config.version %>" rel="stylesheet" />
		<link href="/css/myDialog.css?v=<%= config.version %>" rel="stylesheet" />
		<script src="/lib/sea.js?v=<%= config.version %>"></script>
		<script>
			(function(l){
				var APP = (function(){
					function _APP(){
						var pageName = "<%= pageName %>";
						var urls = {
							web: "http://www.vd.cn"
						};
						this.version = "<%- config.env %>";
						//
						Object.defineProperty(this, "pageName", {
							get: function(){
								return pageName;
							}
						});
						//
						Object.defineProperty(this, "urls", {
							get: function(){
								return urls;
							},
							set: function(o){
								for(var k in o){
									urls[k] = o[k];
								}
							}
						});
					}
					return new _APP();
				})();
				//
				Object.defineProperties(window, {
					APP: {
						value:APP,
						writable: false,
						enumerable:false,
						configurable:false
					},
					"_$":{
						value:{v:"1.0.1"},
						writable: true,
						enumerable:false,
						configurable:false
					},
					"$": {
						get: function(){
							return this["_$"];
						},
						set: function(v){
							console.log("not allow!");
						}
					}
				});
				//
				<% if(1){ %>
				var _base = "/";
				<% }else{ %>
				var _base = "/build";
				l = l||[,"-min"];
					window.onerror= function(err){
						console.log(err);
						return true;
					}
				<%} %>
				seajs.config({
					base: _base,
					map: [
					    [".js", (l&&l[1]||"")+".js"]
					]
				});
			})(location.href.match(/de(\-\d+)bug/));
		</script>
	</head>
	<body onselectstart="return true;" ondragstart="return false;" >